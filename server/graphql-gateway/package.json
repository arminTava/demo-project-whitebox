{
  "name": "installion_tools-gateway",
  "version": "0.0.3",
  "private": true,
  "main": "dist/main.ts",
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/main.ts",
    "dev2": "node src/test/main.js",
    "build": "tsup",
    "start": "ts-node src/main.ts",
    "check": "tsc --pretty --noEmit",
    "postinstall": "npm run generate-prisma",
    "tsc": "tsc -p tsconfig.json",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "test": "npm run test:unit",
    "test-watch": "jest --verbose --watch",
    "test-coverage": "npm run test-coverage:unit",
    "test:unit": "DOTENV_CONFIG_PATH=.env.test jest --setupFiles=dotenv/config --verbose",
    "test-coverage:unit": "DOTENV_CONFIG_PATH=.env.test jest --setupFiles=dotenv/config --coverage --verbose",
    "update-dependencies": "npx npm-check-updates -u",
    "generate-prisma": "prisma generate",
    "generate-graphql": "graphql-codegen",
    "docker-up": "docker compose up -d --wait && sleep 5 && dotenv -e .env.test -- npx prisma migrate dev",
    "docker-down": "docker compose down"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/typescript": "2.8.2",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@graphql-codegen/typescript-resolvers": "2.7.7",
    "@prisma/client": "4.12.0",
    "@prisma/internals": "4.12.0",
    "@prisma/migrate": "4.12.0",
    "@tsconfig/node18": "1.0.1",
    "@types/jest": "29.2.2",
    "@types/mysql": "2.15.21",
    "cross-env": "7.0.3",
    "jest": "29.3.1",
    "jest-environment-node": "29.3.1",
    "jest-junit": "14.0.1",
    "jest-mock-extended": "3.0.1",
    "jest-mysql": "2.0.0",
    "mysql": "2.18.1",
    "prisma": "4.12.0",
    "ts-jest": "29.0.3",
    "ts-node-dev": "2.0.0"
  },
  "dependencies": {
    "@apollo/gateway": "2.3.4",
    "@envelop/apollo-federation": "3.0.6",
    "@envelop/disable-introspection": "^4.0.6",
    "@graphql-tools/schema": "9.0.9",
    "graphql": "16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "3.7.2"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
